	    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
	  </None>
	  <None Include="..\HtmlToPdf.Core\runtimes\win-x86\native\wkhtmltox.dll" Link="runtimes\win-x86\native\wkhtmltox.dll">
