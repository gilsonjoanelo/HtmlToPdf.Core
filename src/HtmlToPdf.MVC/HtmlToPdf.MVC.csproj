	  <None Include="..\HtmlToPdf.Core\runtimes\win-x86\native\libwkhtmltox.a" Link="runtimes\win-x86\native\libwkhtmltox.a">
	    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
